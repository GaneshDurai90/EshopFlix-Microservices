{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithEnvironmentName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  },

  "OpenTelemetry": {
    "AzureMonitor": {
      "ConnectionString": "InstrumentationKey=YOUR-APP-INSIGHTS-INSTRUMENTATION-KEY"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DbConnection": "Data Source=GANESHD;Initial Catalog=CatalogService;Integrated Security=True; TrustServerCertificate=True",
    "LogConnection": "Data Source=GANESHD;Initial Catalog=LoggingService;Integrated Security=True; TrustServerCertificate=True"
  },
  "ApiAddress": {
    "StockApi": "https://localhost:7200/api/"

  },
  "ImageServer": "https://eshopflixcatalog101.blob.core.windows.net"
}
